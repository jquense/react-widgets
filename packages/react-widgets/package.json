  "scripts": {
    "test": "npm run lint && npm run test-only",
    "test-only": "jest --runInBand",
    "tdd": "jest --watch --runInBand",
    "lint": "eslint src test --ext .ts,.tsx,.js",
    "scss": "sass src/scss/styles.scss lib/styles.css",
    "build:pick": "cherry-pick --cwd=lib --input-dir=../src --cjs-dir=cjs --esm-dir=esm",
    "build:lib": "4c build src --no-copy-files && yarn run build:pick && cpy src/scss/* lib/scss/",
    "build:dist": "webpack --mode production && yarn scss",
    "build": "yarn build:lib && yarn build:dist",
    "start": "mini-storybook start",
    "prepublishOnly": "yarn build && file-butler prepare-publish-dir lib",
    "storybook": "mini-storybook start"
  },
  "homepage": "https://jquense.github.io/react-widgets/",
