{
  "name": "react-widgets-globalize",
  "version": "5.0.5",
  "description": "A globalize.js localizer for react-widgets",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist/* lib/*",
    "dist":
      "node ../../tools/build-localizer.js '{\"globalize\": \"window.Globalize\"}'",
    "build": "babel localizer.js -o index.js",
    "prepublishOnly": "npm run clean && npm run build && npm run dist"
  },
  "files": ["index.js", "dist"],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jquense/react-widgets.git"
  },
  "keywords": ["react-widgets"],
  "author": "Jason Quense",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jquense/react-widgets/issues"
  },
  "homepage": "https://github.com/jquense/react-widgets#readme",
  "peerDependencies": {
    "globalize": ">=0.1.0",
    "react": ">=0.14.0",
    "react-widgets": "^4.2.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.42",
    "@babel/core": "^7.0.0-beta.42",
    "cross-env": "^5.1.4",
    "react-widgets": "^4.2.5",
    "rimraf": "^2.6.2"
  },
  "dependencies": {
    "prop-types": "^15.6.1"
  }
}
