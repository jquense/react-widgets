{
  "name": "react-widgets",
  "version": "4.0.0-beta.3",
  "description": "An Ã  la carte set of polished, extensible, and accessible inputs built for React",
  "main": "lib/index.js",
  "author": {
    "name": "Jason",
    "email": "monastic.panic@gmail.com"
  },
  "keywords": [
    "react",
    "widgets",
    "dropdown",
    "combobox",
    "calendar",
    "datepicker",
    "date picker",
    "numberpicker",
    "number picker",
    "radio group",
    "checkbox list",
    "multiselect",
    "form",
    "input",
    "react-ui",
    "react-component"
  ],
  "scripts": {
    "test": "npm run lint && npm run test:all",
    "test:all": "npm run test:server && npm run test:client",
    "test:server": "cross-env NODE_ENV=test mocha -R spec ./test/server.js",
    "test:client": "cross-env NODE_ENV=test karma start --single-run=true",
    "tdd": "cross-env NODE_ENV=test karma start",
    "lint": "eslint src test --ext .js",
    "to-sass": "node ./build/lessToSass.js",
    "less": "lessc -x --autoprefix=\"ie >= 9\" src/less/react-widgets.less dist/css/react-widgets.css",
    "styles": "cpy src/less/* lib/less/ && npm run to-sass",
    "assets:lib": "cpy src/img/* lib/img/ && cpy src/fonts/* lib/fonts/",
    "assets:dist": "cpy src/img/* dist/img/ && cpy src/fonts/* dist/fonts/",
    "clean:lib": "rimraf ./lib/*",
    "clean:dist": "rimraf ./dist/*",
    "clean:docs": "rimraf ./docs/public/*",
    "compile:lib": "babel src --out-dir lib",
    "compile:docs": "cross-env NODE_ENV=docs webpack --config build/webpack.config.js",
    "compile:dist": "cross-env NODE_ENV=dist webpack --config build/webpack.config.js",
    "build:lib": "npm run clean:lib && npm run compile:lib && npm run styles && npm run assets:lib",
    "build:docs": "npm run clean:docs && npm run compile:docs",
    "build:dist": "npm run clean:dist && npm run compile:dist && npm run less && npm run assets:dist",
    "build": "npm run build:lib && npm run build:dist",
    "docs": "npm run clean:docs && cross-env NODE_ENV=docs webpack-dev-server --config ./build/webpack.config.js --inline",
    "dev": "start-storybook -p 9001",
    "release": "release",
    "alpha": "release --preid alpha --tag next",
    "beta": "release --preid beta --tag next"
  },
  "homepage": "http://jquense.github.io/react-widgets/docs/",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jquense/react-widgets"
  },
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0",
    "react-dom": "^0.14.0 || ^15.0.0"
  },
  "dependencies": {
    "classnames": "^2.1.2",
    "date-arithmetic": "^3.0.0",
    "dom-helpers": "^2.2.4",
    "invariant": "^2.1.0",
    "loose-envify": "^1.2.0",
    "spy-on-component": "^1.0.2",
    "uncontrollable": "^4.0.0",
    "warning": "^2.0.0"
  },
  "optionalDependencies": {
    "format-number-with-string": "0.0.2",
    "deconstruct-number-format": "0.0.1"
  },
  "devDependencies": {
    "@kadira/storybook": "^1.39.0",
    "@monastic.panic/component-playground": "^2.0.0",
    "babel-cli": "^6.7.5",
    "babel-core": "^6.7.6",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-jason": "^1.0.1",
    "bill": "^3.2.2",
    "bootstrap": "^3.3.7",
    "chance": "^1.0.1",
    "common-tags": "^1.3.1",
    "cpy-cli": "^1.0.0",
    "cross-env": "^2.0.0",
    "css-loader": "^0.23.1",
    "es5-shim": "^4.4.1",
    "eslint": "^3.7.1",
    "eslint-config-jason": "^3.0.0",
    "eslint-plugin-import": "^2.0.0",
    "eslint-plugin-mocha": "^4.6.0",
    "eslint-plugin-react": "^6.3.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.1",
    "glob": "^7.0.5",
    "globalize": "0.1.x",
    "imports-loader": "^0.6.4",
    "json-loader": "^0.5.3",
    "karma": "^0.13.3",
    "karma-chrome-launcher": "^0.2.1",
    "karma-expect": "~1.1.0",
    "karma-jsdom-launcher": "^3.0.0",
    "karma-mocha": "^0.2.0",
    "karma-mocha-reporter": "^2.0.0",
    "karma-sauce-launcher": "^0.3.1",
    "karma-sinon": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.5",
    "karma-webpack": "^1.7.0",
    "less": "^2.5.1",
    "less-loader": "^2.2.0",
    "less-plugin-autoprefix": "^1.5.1",
    "less2sass": "^1.0.2",
    "marked": "^0.3.5",
    "mkdirp": "^0.5.1",
    "mocha": "^2.3.4",
    "moment": "^2.10.6",
    "mt-changelog": "^0.6.2",
    "node-libs-browser": "^1.0.0",
    "output-file-sync": "^1.1.1",
    "prismjs": "^1.2.0",
    "raw-loader": "^0.5.1",
    "react": "^15.2.0",
    "react-bootstrap": "^0.29.0-0",
    "react-dom": "^15.2.0",
    "react-hot-loader": "^3.0.0-beta.0",
    "react-router": "2.0.1",
    "react-router-bootstrap": "^0.23.1",
    "release-script": "^1.0.2",
    "rimraf": "^2.4.2",
    "scriptjs": "^2.5.8",
    "sinon": "^1.17.2",
    "style-loader": "^0.13.1",
    "teaspoon": "^6.4.1",
    "uncontrollable": "^3.2.3",
    "url-loader": "^0.5.5",
    "webpack": "^1.10.5",
    "webpack-dev-server": "^1.10.1",
    "webpack-merge": "^0.14.0",
    "yargs": "^4.4.0"
  },
  "browserify": {
    "transform": [
      "loose-envify"
    ]
  }
}
